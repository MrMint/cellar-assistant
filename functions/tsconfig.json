{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strictNullChecks": false
  }
}
